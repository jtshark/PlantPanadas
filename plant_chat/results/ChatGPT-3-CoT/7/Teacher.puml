@startuml
class Teacher {
  - name: String
  - email: String
  - password: String
  - classes: List<Class>
  + scheduleClass(title: String, description: String, startDateTime: DateTime, endDateTime: DateTime): void
  + shareMaterial(class: Class, material: Material): void
  + evaluateWork(student: Student, assignment: Assignment): void
}
@enduml