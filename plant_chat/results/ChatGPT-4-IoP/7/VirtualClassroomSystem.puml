@startuml
class Teacher {
  +scheduleClasses()
  +shareMaterials()
  +evaluateStudentWork()
}

class Student {
  +attendClasses()
  +submitAssignments()
  +communicate()
}

class Classroom {
  -teachers: Teacher[]
  -students: Student[]
  +addTeacher(teacher: Teacher)
  +addStudent(student: Student)
}

Teacher "1" -- "0..*" Classroom : teaches
Student "1" -- "0..*" Classroom : attends
@enduml