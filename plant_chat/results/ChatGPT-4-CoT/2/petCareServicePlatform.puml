@startuml

class PetOwner {
  name
  address
  petDetails
  bookService()
  cancelService()
}

class ServiceProvider {
  name
  serviceType
  price
  setPrice()
  manageSchedule()
}

class Service {
  serviceName
  serviceDescription
  serviceDuration
  getDescription()
  getDuration()
}

class Schedule {
  date
  time
  bookedStatus
  bookTimeSlot()
  cancelTimeSlot()
}

PetOwner "1" -- "many" Service : books
Service "many" -- "1" ServiceProvider : provided by
ServiceProvider "1" -- "1" Schedule : has

@enduml